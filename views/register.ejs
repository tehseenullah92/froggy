<body class="fixed-header menu-pin">
		<div class="login-wrapper ">
			<div class="bg-pic">
				<img src="/img/demo/new-york-city-buildings-sunrise-morning-hd-wallpaper.jpg" data-src="/img/demo/new-york-city-buildings-sunrise-morning-hd-wallpaper.jpg" data-src-retina="/img/demo/new-york-city-buildings-sunrise-morning-hd-wallpaper.jpg" alt="" class="lazy">
				<div class="bg-caption pull-bottom sm-pull-bottom text-white p-l-20 m-b-20">
					<p class="small">
						Â© 2018 Froggy.
					</p>
				</div>
			</div>
			<div class="login-container bg-white">
				<div class="p-l-50 m-l-20 p-r-50 m-r-20 p-t-50 m-t-30 sm-p-l-15 sm-p-r-15 sm-p-t-40">
          <img src="/img/logo.png" class="text-center" alt="logo" data-src="/img/logo.png" data-src-retina="/img/logo_2x.png" width="100" height="100">
					<p class="p-t-35">Register Here</p>
					<form id="form-login" class="p-t-15" method="post" role="form" action="/users/register">
						<div class="form-group form-group-default">
							<label>Full Name</label>
							<div class="controls">
								<input type="text" name="name" placeholder="Full Name" class="form-control" required>
							</div>
						</div>
            <div class="form-group form-group-default">
							<label>Username</label>
							<div class="controls">
								<input type="text" name="username" placeholder="Username" class="form-control" required>
							</div>
						</div>
						<div class="form-group form-group-default">
							<label>Email</label>
							<div class="controls">
								<input type="text" class="form-control" name="email" placeholder="Email" required>
							</div>
						</div>
            <div class="form-group form-group-default">
							<label>Password</label>
							<div class="controls">
								<input type="password" class="form-control" name="password" placeholder="Password" required>
							</div>
						</div>
            <div class="row">
							<div class="col-md-12 d-flex align-items-center justify-content-end">
                <div class="col-xs-6">
                  <a href="/users/login" class="text-info small pull-right">Sign In</a>
                </div>
                <div class="col-xs-6">
                  <a href="/users/forgot" class="text-info small">Forgot Password</a>
                </div>
							</div>
						</div>
						<!-- END Form Control-->
						<button class="btn btn-primary btn-cons m-t-10" type="submit">Sign Up</button>
					</form>
				</div>
			</div>
		</div>
